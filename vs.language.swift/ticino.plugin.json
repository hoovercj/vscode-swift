{
	"pluginId": "vs.language.swift",
	"activationEvents": ["textModel:swift"],
	"mainModule": "./swiftMain",
	"scripts": {
		"compile": "node ../../node_modules/gulp/bin/gulp.js --gulpfile ../gulpfile.js compile --dir vs.language.swift"
	},
	"contributes": {
		"language": [{
			"id": "swift",
			"aliases": ["Swift","swift"],
			"extensions": [".swift"],
			"mimetypes": ["text/swift"]
		}]
	},
	"_bundling": [
		{ "name": "./swiftMain" }
	]
}